<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bckimage">

    <TextView
        android:id="@+id/textView30"
        android:layout_width="412dp"
        android:layout_height="606dp"
        android:background="#70FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-3dp" />

    <EditText
        android:id="@+id/editTextPhone6"
        android:layout_width="175dp"
        android:layout_height="28dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="209dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:text="  CONTACT US"
        android:textColor="#000000"

        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="110dp"
        android:layout_height="32dp"
        android:layout_marginTop="32dp"
        android:text=" DOCTOR"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="170dp"
        android:layout_height="28dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone2"
        android:layout_width="169dp"
        android:layout_height="29dp"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/editTextPhone"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="151dp"
        android:layout_height="41dp"
        android:layout_marginTop="24dp"
        android:text=" PHARMACY"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone7"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone3"
        android:layout_width="164dp"
        android:layout_height="27dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="181dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:text=" OUR CONTACT"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone9"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone4"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone6"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone7"
        android:layout_width="171dp"
        android:layout_height="28dp"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/editTextPhone2"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone8"
        android:layout_width="163dp"
        android:layout_height="27dp"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/editTextPhone3"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone3"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextPhone9"
        android:layout_width="163dp"
        android:layout_height="30dp"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toStartOf="@+id/editTextPhone8"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone8"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/backHomeContact"
        android:layout_width="111dp"
        android:layout_height="48dp"
        android:background="@drawable/custom_button2"
        android:text="Back to Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

</androidx.constraintlayout.widget.ConstraintLayout>