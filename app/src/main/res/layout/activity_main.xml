<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/notification"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:text="notification"
        app:layout_constraintStart_toEndOf="@+id/profile"
        app:layout_constraintTop_toTopOf="@+id/profile"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/trackers"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginTop="28dp"
        android:text="trackers"
        app:layout_constraintStart_toStartOf="@+id/history"
        app:layout_constraintTop_toBottomOf="@+id/history"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/calendar"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:text="calender"
        app:layout_constraintEnd_toEndOf="@+id/notification"
        app:layout_constraintStart_toEndOf="@+id/history"
        app:layout_constraintTop_toBottomOf="@+id/notification"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/contact"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="28dp"
        android:text="contact"
        app:layout_constraintEnd_toEndOf="@+id/calendar"
        app:layout_constraintStart_toEndOf="@+id/trackers"
        app:layout_constraintTop_toBottomOf="@+id/calendar"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/profile"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginTop="144dp"
        android:text="Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/help"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="28dp"
        android:text="help"
        app:layout_constraintBottom_toBottomOf="@+id/setting"
        app:layout_constraintEnd_toEndOf="@+id/contact"
        app:layout_constraintStart_toEndOf="@+id/setting"
        app:layout_constraintTop_toBottomOf="@+id/contact"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/setting"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginTop="28dp"
        android:text="settings"
        app:layout_constraintStart_toStartOf="@+id/trackers"
        app:layout_constraintTop_toBottomOf="@+id/trackers"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/history"
        android:layout_width="132dp"
        android:layout_height="101dp"
        android:layout_marginTop="32dp"
        android:text="history"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintTop_toBottomOf="@+id/profile"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>

