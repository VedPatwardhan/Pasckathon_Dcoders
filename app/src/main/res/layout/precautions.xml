<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bckimage">

    <TextView
        android:id="@+id/textView30"
        android:layout_width="412dp"
        android:layout_height="606dp"
        android:background="#70FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-3dp" />

    <Button
        android:id="@+id/typhoid"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="52dp"
        android:text="TYPHOID"
        android:background="@drawable/custom_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tetanus"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_button"
        android:text="TETANUS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/typhoid" />

    <Button
        android:id="@+id/diabetes"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/custom_button"
        android:text="DIABETES"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tetanus" />

    <Button
        android:id="@+id/jaundice"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_button"
        android:text="JAUNDICE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/diabetes" />

    <Button
        android:id="@+id/livercirrhosis"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/custom_button"
        android:text="LIVER CIRRHOSIS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jaundice" />

    <Button
        android:id="@+id/pneumonia"
        android:layout_width="265dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/custom_button"
        android:text="PNEUMONIA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/livercirrhosis" />

    <Button
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK TO HOME"
        android:background="@drawable/custom_button"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="530dp" />


</androidx.constraintlayout.widget.ConstraintLayout>